model{

    #The likelihood:

    for (i in 1:length(x)){
        S[i] <- (A * exp((-(x[i]-x0)^2) / ( 2 *w^2)) + B)

        y[i] ~ dpois(S[i]) 
    } 


    # Priors for  A, B, x0, w

    A ~ dnorm( 140317 , 50 )
    B ~ dnorm( 1517 , 10 )

    x0 ~ dunif( 1449 , 1549 )
    w ~ dunif(1,10)
      
}